# Environment variables for Starboard Write Server
NODE_ENV=development
PORT=5001
CLIENT_URL=http://localhost:3000

# Database
MONGODB_URI=mongodb://localhost:27017/starboard-write

# JWT Secret (change in production)
JWT_SECRET=starboard_write_jwt_secret_key_change_in_production

# AI Service (OpenAI or other)
OPENAI_API_KEY=your_openai_api_key_here
AI_MODEL=gpt-3.5-turbo

# Session secret
SESSION_SECRET=starboard_write_session_secret_change_in_production
